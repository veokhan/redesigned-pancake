modules = ["nodejs-20"]
[agent]
expertMode = true

[nix]
channel = "stable-25_05"

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "dev"

[[workflows.workflow]]
name = "dev"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "npm run dev"
waitForPort = 5000

[workflows.workflow.metadata]
outputType = "webview"

[[ports]]
localPort = 5000
externalPort = 80

[[ports]]
localPort = 5001
externalPort = 3002

[[ports]]
localPort = 36469
externalPort = 5173

[[ports]]
localPort = 36763
externalPort = 3001

[[ports]]
localPort = 43157
externalPort = 3003

[[ports]]
localPort = 44349
externalPort = 5000

[[ports]]
localPort = 46087
externalPort = 4200

[[ports]]
localPort = 46381
externalPort = 3000

[deployment]
deploymentTarget = "autoscale"
run = ["npm", "run", "dev"]
build = ["npm", "run", "build:web"]
